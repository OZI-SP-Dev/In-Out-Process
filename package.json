  "dependencies": {
    "@fluentui/example-data": "^8.4.12",
    "@fluentui/react": "^8.112.3",
    "@fluentui/react-components": "^9.36.0",
    "@fluentui/react-icons": "^2.0.221",
    "@fluentui/react-icons-mdl2": "^1.3.50",
