{
  "name": "In-Out-Process",
  "version": "1.1.0",
  "private": true,
  "homepage": ".",
  "type": "module",
  "dependencies": {
    "@fluentui/example-data": "^8.4.6",
    "@fluentui/react": "^8.105.5",
    "@fluentui/react-components": "^9.14.0",
    "@fluentui/react-icons": "^2.0.193",
    "@fluentui/react-icons-mdl2": "^1.3.31",
    "@pnp/core": "^3.11.0",
    "@pnp/sp": "^3.11.0",
    "@tanstack/react-query": "^4.24.4",
    "dompurify": "^2.4.3",
    "env-cmd": "^10.1.0",
    "luxon": "^3.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.43.1",
    "react-router-dom": "^6.8.1"
  },
  "scripts": {
    "build:test": "tsc --noEmit && vite build --mode testing && npm run mergeaspx",
    "build:prod": "tsc --noEmit && vite build && npm run mergeaspx",
    "dev": "vite --open",
    "mergeaspx": "concat -o dist/index.aspx dist/index.aspx dist/index.html",
    "test": "env-cmd -f ./.env.test.json react-scripts test --verbose --transformIgnorePatterns \"node_modules/(?!@fluentui)/\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/dompurify": "^2.4.0",
    "@types/jest": "^29.4.0",
    "@types/luxon": "^3.2.0",
    "@types/node": "^16.18.12",
    "@types/react": "^17.0.53",
    "@types/react-dom": "^17.0.18",
    "@vitejs/plugin-react": "3.1.0",
    "concat": "^1.0.3",
    "msw": "^1.0.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vite-tsconfig-paths": "^4.0.5"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
